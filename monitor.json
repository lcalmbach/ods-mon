{
    "100051": {
        "root": "data.bs.ch",
        "dataset": "100051",
        "title": "Luftqualit\u00e4t Station Basel-Binningen",
        "timestamp-col": "datum_zeit",
        "include-is-weekday": false,
        "value-cols": {
            "o3_ug_m3": {
                "name": "O3",
                "agg_funcs": [
                    "mean",
                    "max",
                    "min"
                ],
                "standards": []
            },
            "no2_ug_m3": {
                "name": "NO2",
                "agg_funcs": [
                    "mean",
                    "max",
                    "min"
                ],
                "standards": []  
            },
            "pm10_ug_m3": {
                "name": "PM10",
                "agg_funcs": [
                    "mean",
                    "max",
                    "min"
                ],
                "standards": []
            },
            "pm2_5_ug_m3": {
                "name": "PM2.5",
                "agg_funcs": [
                    "mean",
                    "max",
                    "min"
                ],
                "standards": []
            }
        },
        "aggregation": "d",
        "rename-cols": false,
        "reports": {
            "day":
                {
                    "compare_to_periods": ["last_month", "year_todate", "all_time"],
                    "parameters": ["o3_ug_m3_mean", "o3_ug_m3_max", "no2_ug_m3_mean", "no2_ug_m3_max", "pm10_ug_m3_mean", "pm10_ug_m3_max", "pm2_5_ug_m3_mean", "pm2_5_ug_m3_max"]
                },
            "month":
                {
                    "compare_to_periods": ["last_year", "all_time"],
                    "parameters": ["o3_ug_m3_mean", "o3_ug_m3_max", "no2_ug_m3_mean", "no2_ug_m3_max", "pm10_ug_m3_mean", "pm10_ug_m3_max", "pm2_5_ug_m3_mean", "pm2_5_ug_m3_max"]
                },
            "year":
                {
                    "compare_to_periods": ["last_year", "all_time"],
                    "parameters": ["o3_ug_m3_mean", "o3_ug_m3_max", "no2_ug_m3_mean", "no2_ug_m3_max", "pm10_ug_m3_mean", "pm10_ug_m3_max", "pm2_5_ug_m3_mean", "pm2_5_ug_m3_max"]
